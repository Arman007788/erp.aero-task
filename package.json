    "@types/jsonwebtoken": "^9.0.2",
    "@types/node-cron": "^3.0.8",
    "@types/paypal-rest-sdk": "^1.7.6",
    "@types/validator": "^13.7.17",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
