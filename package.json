{
  "name": "test-task",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "node build/app.js",
    "dev": "nodemon --config nodemon.json && ts-node/register src/app.ts",
    "build": "npx tsc",
    "lint:fix": "eslint . --ext .ts --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm -- -d ./src/configs/db.config.ts migration:generate -- src/migrations/$npm_config_name",
    "migration:run": "npm run typeorm  migration:run -- -d ./src/config/data-source.ts",
    "migration:revert": "npm run typeorm -- -d ./src/config/data-source.ts migration:revert"
  },
